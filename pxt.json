{
    "name": "pxt-wappsto",
    "version": "1.1.0",
    "description": "MakeCode extension for Wappsto:bit IoT module",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "README.md",
        "wappsto.ts",
        "microbit.ts",
        "calliope.ts",
        "_locales/en/pxt-wappsto-jsdoc-strings.json",
        "_locales/en/pxt-wappsto-strings.json",
        "_locales/da/pxt-wappsto-jsdoc-strings.json",
        "_locales/da/pxt-wappsto-strings.json",
        "_locales/de/pxt-wappsto-jsdoc-strings.json",
        "_locales/de/pxt-wappsto-strings.json"
    ],
    "testFiles": [
        "wappstotest.ts"
    ],
    "fileDependencies": {
        "microbit.ts": "target:microbit",
        "calliope.ts": "target:calliopemini"
    },
    "public": true,
    "supportedTargets": [
        "microbit",
        "calliope"
    ],
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "enabled": 0
                }
            }
        }
    }
}
